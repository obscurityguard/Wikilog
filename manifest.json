{
  "manifest_version": 2,

  "name": "WikiLog",
  "description": "This extension logs a user's Wikipedia history.",
  "version": "1.0",
  "background": {
      "scripts": ["eventPage.js"],
      "persistent": false
    },
  "browser_action": {
    "default_icon": "new_icon.png",
    "default_popup": "popup.html",
    "default_title": "WikiLog"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "activeTab",
    "storage"
  ]
}
